<?xml version="1.0"?>
<doc>
    <assembly>
        <name>Backpack.Net</name>
    </assembly>
    <members>
        <member name="T:Backpack.Net.BackpackClient">
            <summary>
            A client for backpack.tf granting access to its APIs.
            </summary>
        </member>
        <member name="M:Backpack.Net.BackpackClient.#ctor(System.String)">
            <summary>
            Creates a new instance of a <see cref="T:Backpack.Net.BackpackClient"/>.
            </summary>
            <param name="apiKey">Your backpack.tf API key, obtainable from https://backpack.tf/developer/apikey/view .</param>
        </member>
        <member name="M:Backpack.Net.BackpackClient.GetCurrenciesAsync(Backpack.Net.CurrencyValue)">
            <summary>
            Gets information about the currencies used by backpack.tf.
            </summary>
            <param name="value">The type(s) of price values desired to be included in the response.</param>
        </member>
        <member name="M:Backpack.Net.BackpackClient.GetItemPricesAsync(Backpack.Net.CurrencyValue,System.Nullable{System.DateTimeOffset})">
            <summary>
            Gets current price information for all items. 
            </summary>
            <param name="value">The type(s) of price values desired to be included in the response.</param>
            <param name="since">If set, only items with prices newer than this <see cref="T:System.DateTimeOffset"/> will be returned.</param>
        </member>
        <member name="M:Backpack.Net.BackpackClient.GetPriceHistoryAsync(System.String,Backpack.Net.Quality,System.Boolean,Backpack.Net.PriceIndex)">
            <summary>
            Gets price history information for an item.
            </summary>
            <param name="itemName">The name of the item. (Exact match only)</param>
            <param name="quality">The <see cref="T:Backpack.Net.Quality"/> of the item.</param>
            <param name="craftable">Whether to search for the Craftable or Non-Craftable variant of the item.</param>
            <param name="priceIndex">A specific <see cref="T:Backpack.Net.PriceIndex"/> for the item.</param>
        </member>
        <member name="M:Backpack.Net.BackpackClient.GetImpersonatedUsersAsync(System.Int32,System.Int32)">
            <summary>
            Gets a list of frequently impersonated users on backpack.tf.
            </summary>
            <param name="limit">The limit of users to fetch. Used for pagination.</param>
            <param name="skip">The number of users to "skip" before fetching. Used for pagination.</param>
        </member>
        <member name="M:Backpack.Net.BackpackClient.GetUsersAsync(System.UInt64[])">
            <summary>
            Gets various backpack.tf user information.
            </summary>
            <param name="steamIds">A collection of SteamID64s.</param>
        </member>
        <member name="M:Backpack.Net.BackpackClient.Dispose">
            <inheritdoc />
        </member>
        <member name="T:Backpack.Net.CurrencyValue">
            <summary>
            Represents additional currency values able to be requested for endpoints which return <see cref="T:Backpack.Net.Price"/> models.
            </summary>
        </member>
        <member name="F:Backpack.Net.CurrencyValue.None">
            <summary>
            The <see cref="T:Backpack.Net.Price"/> model will only display its value.
            </summary>
        </member>
        <member name="F:Backpack.Net.CurrencyValue.Raw">
            <summary>
            The <see cref="T:Backpack.Net.Price"/> model will carry a raw value, which is the value in the lowest currency without rounding.
            </summary>
        </member>
        <member name="F:Backpack.Net.CurrencyValue.RawHigh">
            <summary>
            The <see cref="T:Backpack.Net.Price"/> model will carry a raw value and raw high value, which is the upper bound of the average value in the lowest currency without rounding.
            </summary>
        </member>
