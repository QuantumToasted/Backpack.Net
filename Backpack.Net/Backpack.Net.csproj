<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Daniel Jarski</Authors>
    <Product />
    <Description>A REST API wrapper for (most of) https://backpack.tf 's endpoints.</Description>
    <PackageProjectUrl>https://gitlab.com/QuantumToast/Backpack.Net/</PackageProjectUrl>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageTags>tf2 rest api</PackageTags>
    <RepositoryUrl>https://gitlab.com/QuantumToast/Backpack.Net/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Copyright>See https://gitlab.com/QuantumToast/Backpack.Net/blob/master/LICENSE</Copyright>
    <PackageLicenseUrl>https://gitlab.com/QuantumToast/Backpack.Net/blob/master/LICENSE</PackageLicenseUrl>
    <PackageReleaseNotes>Added all functional (and useful) REST endpoints:
IGetCurrencies/v1
IGetPriceHistory/v1
IGetPrices/v4
IGetUsers/GetImpersonatedUsers
users/info/v1</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>1701;1702;CS0649</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="System.Collections.Immutable" Version="1.6.0-preview.19073.11" />
  </ItemGroup>

</Project>
